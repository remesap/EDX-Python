   "source": [
    "def hanoi(n, start, end):\n",
    "    if n == 1:\n",
    "        pm(start, end) \n",
    "    else:\n",
    "        other = 6 -(start + end)\n",
    "        hanoi(n-1, start, other)\n",
    "        pm(start, end)\n",
    "        hanoi(n-1, other, end)\n",
    "    \n",
    "    \n",
    "def pm(start, end):\n",
    "    print(start, \" --> \", end)\n",
    "\n",
    "hanoi(3, 1, 3)\n"
   ]
  }
